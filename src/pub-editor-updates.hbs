<div class="editorupdates">
<h3>{{name}}</h3>
<ul class="difflist">
{{#each diffs}}
<li title="Click to open in editor.{{difftext}}" data-file="{{file}}" data-href="{{diffpage}}" class="difflistitem">
  <span class="diffrevert" title="Click to revert to published content (will trigger editor reload.)">𐄂</span>
  <span class="diffcommit" title="Click to commit and publish changes.">✔︎</span>
  {{difffragments}}
</li>
{{/each}}
</ul>
</div>
